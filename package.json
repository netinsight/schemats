{
  "name": "schemats",
  "version": "3.0.10",
  "description": "Generate typescript interface definitions from (postgres) SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json --type-check 'src/**/*.ts' 'test/**/*.test.ts' 'bin/**/*.ts' --exclude '**/*.d.ts'",
    "build": "tsc",
    "dependency-check": "dependency-check . --entry dist/bin/schemats.js --missing --no-dev",
    "test": "npm run test:unit",
    "test:unit": "mocha dist/test/unit",
    "prepublish": "npm run build",
    "coverage": "nyc mocha"
  },
  "bin": {
    "schemats": "dist/bin/schemats.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sweetiq/schemats.git"
  },
  "bugs": {
    "url": "https://github.com/sweetiq/schemats/issues"
  },
  "author": "Mengxuan Xia <xiamx2004@gmail.com>",
  "contributors": [
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/diff": "^3.5.8",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^2.2.48",
    "@types/mysql": "0.0.34",
    "@types/node": "^20.11.27",
    "@types/power-assert": "^1.5.12",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^2.3.7",
    "@types/yargs": "^6.6.0",
    "dependency-check": "^2.6.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1",
    "proxyquire": "^1.8.0",
    "sinon": "^2.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^3.3.0",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^5.0.2"
  },
  "dependencies": {
    "diff": "^3.5.0",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "pg-promise": "^10.15.4",
    "typescript": "^4.9.5",
    "typescript-formatter": "^7.2.2",
    "yargs": "^16.2.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
