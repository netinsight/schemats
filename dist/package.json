{
    "name": "schemats",
    "version": "3.0.10",
    "description": "Generate typescript interface definitions from (postgres) SQL database schema",
    "keywords": [
        "postgres",
        "schema",
        "typescript",
        "sql"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "lint": "tslint --project tsconfig.json --type-check 'src/**/*.ts' 'test/**/*.test.ts' 'bin/**/*.ts' --exclude '**/*.d.ts'",
        "build": "tsc",
        "dependency-check": "dependency-check . --entry dist/bin/schemats.js --missing --no-dev",
        "test": "npm run test:unit",
        "test:unit": "mocha dist/test/unit",
        "prepublish": "npm run build",
        "coverage": "nyc mocha"
    },
    "bin": {
        "schemats": "dist/bin/schemats.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sweetiq/schemats.git"
    },
    "bugs": {
        "url": "https://github.com/sweetiq/schemats/issues"
    },
    "author": "Mengxuan Xia <xiamx2004@gmail.com>",
    "contributors": [
        "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
        "zigomir <zigomir@gmail.com>",
        "Mark Crisp <macr1324@gmail.com>"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/diff": "^3.2.0",
        "@types/lodash": "^4.14.37",
        "@types/mocha": "^2.2.39",
        "@types/mysql": "0.0.33",
        "@types/node": "^16.9.2",
        "@types/power-assert": "^1.4.29",
        "@types/proxyquire": "^1.3.27",
        "@types/sinon": "^2.1.2",
        "@types/yargs": "^6.3.3",
        "dependency-check": "^2.6.0",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "power-assert": "^1.4.2",
        "proxyquire": "^1.7.11",
        "sinon": "^2.1.0",
        "source-map-support": "^0.4.15",
        "ts-node": "^3.0.4",
        "tslint": "^5.4.2",
        "tslint-config-standard": "^5.0.2"
    },
    "dependencies": {
        "diff": "^3.2.0",
        "lodash": "^4.17.20",
        "mysql": "^2.13.0",
        "pg-promise": "^10.11.0",
        "typescript": "^4.4.3",
        "typescript-formatter": "^7.0.1",
        "yargs": "^16.0.3"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
